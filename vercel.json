{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "main.py": {
            "maxDuration": 10,
            "memory": 512
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
